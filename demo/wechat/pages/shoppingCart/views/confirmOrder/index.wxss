.lang {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.flex {
  display: flex;
}
/* 居中 */
.flexCentered {
  justify-content: center;
}
/* 垂直 */
.flexVertical {
  align-items: center;
}
.flexFirst {
  flex-grow: 0;
}
.flexCenter {
  flex-grow: 1;
}
.flexLast {
  flex-grow: 0;
}
.flexLevel {
  flex: 1;
}
.receiptInfo {
  padding-top: 24rpx;
}
.receiptInfo .receipInfoContainer {
  padding: 37rpx 18rpx 41rpx 28rpx;
  background: white;
  position: relative;
}
.receiptInfo .receipInfoContainer .ic_more {
  position: absolute;
  width: 10rpx;
  height: 16rpx;
  top: 50%;
  margin-top: -8rpx;
  right: 18rpx;
}
.receiptInfo .receipInfoContainer .icon image {
  width: 32rpx;
  height: 32rpx;
}
.receiptInfo .receipInfoContainer .receipInfoContent {
  padding-left: 20rpx;
  padding-right: 20rpx;
  font-size: 24rpx;
  color: #666666;
}
.receiptInfo .receipInfoContainer .receipInfoContent .userInfo {
  padding-bottom: 32rpx;
  font-size: 28rpx;
  color: #333333;
  display: flex;
}
.receiptInfo .receipInfoContainer .receipInfoContent .userInfo .userName {
  flex-grow: 1;
}
.receiptInfo .receipInfoContainer .receipInfoContent .userInfo .userPhone {
  flex-grow: 0;
}
.receiptInfo .receipInfoContainer .receipInfoContent .receipCompany {
  padding-bottom: 22rpx;
}
.receiptInfo .receipInfoContainer .receipInfoContent .receipAddress {
  line-height: 36rpx;
}
.ic_add_list {
  font-size: 0;
  width: 100%;
  height: 14rpx;
}
.orderList .moneyColor {
  color: #FF5B29;
}
.orderList .commonP {
  padding: 0 51rpx 0 30rpx;
}
.orderList .orderListContainer {
  margin-top: 20rpx;
  background: white;
}
.orderList .orderListContainer .orderCommonTitle {
  padding: 0 51rpx 0 30rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #EDEDED;
}
.orderList .orderListContainer .orderCommonTitle .title {
  flex-grow: 1;
  line-height: 100rpx;
  padding-left: 20rpx;
  position: relative;
  font-size: 28rpx;
  color: #333333;
}
.orderList .orderListContainer .orderCommonTitle .title:after {
  position: absolute;
  content: " ";
  width: 4rpx;
  height: 24rpx;
  background: #01a159;
  border-radius: 2rpx;
  top: 50%;
  left: 0;
  margin-top: -12rpx;
}
.orderList .orderListContainer .orderCommonTitle .subtitle {
  flex-grow: 0;
  padding-right: 39rpx;
  font-size: 24rpx;
  color: #999999;
  background: url("http://192.168.1.40:8889/youxinstatic/img/images/tabbar/ic_list_up.png") no-repeat 100% 50%;
  background-size: 15rpx 10rpx;
}
.orderList .orderform {
  font-size: 24rpx;
  color: #333333;
  padding: 0 51rpx 0 30rpx;
}
.orderList .orderform .orderformContainer {
  border-bottom: 1px solid #EDEDED;
}
.orderList .orderform .orderformContent {
  padding: 31rpx 0;
}
.orderList .orderform view {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.orderList .orderform .center {
  text-align: center;
}
